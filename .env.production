# Production Environment Configuration for Railway
# This file contains production-specific environment variable settings

# Application Configuration
ENVIRONMENT=production
DEBUG=false
LOG_LEVEL=INFO
LOG_FORMAT=json

# API Configuration - Railway domain
ALLOWED_ORIGINS=["https://linkedin-ingestion-production.up.railway.app","https://api.personal.direct.burke.solutions"]

# Cassidy AI Configuration (production endpoints)
CASSIDY_PROFILE_WORKFLOW_URL=https://app.cassidyai.com/api/webhook/workflows/cmbv9eaaz00139pa7krnjmoa0?results=true
CASSIDY_COMPANY_WORKFLOW_URL=https://app.cassidyai.com/api/webhook/workflows/cmckultwk008bknkgisuk5fef?results=true
CASSIDY_TIMEOUT=300
CASSIDY_MAX_RETRIES=3
CASSIDY_BACKOFF_FACTOR=2.0

# Database Configuration (Railway PostgreSQL)
# These will be set as Railway service variables
DATABASE_URL=${{DATABASE_URL}}
SUPABASE_URL=${{SUPABASE_URL}}
SUPABASE_ANON_KEY=${{SUPABASE_ANON_KEY}}
SUPABASE_SERVICE_KEY=${{SUPABASE_SERVICE_KEY}}

# Vector Configuration
VECTOR_DIMENSION=1536
SIMILARITY_THRESHOLD=0.8

# OpenAI Configuration
OPENAI_API_KEY=${{OPENAI_API_KEY}}
OPENAI_DEFAULT_MODEL=gpt-3.5-turbo
OPENAI_MAX_TOKENS=2000
OPENAI_TEMPERATURE=0.1

# Rate Limiting (production values)
RATE_LIMIT_PER_MINUTE=100
CASSIDY_RATE_LIMIT=20

# Security (production API key)
API_KEY=${{API_KEY}}

# Feature Flags (production settings)
ENABLE_COMPANY_INGESTION=true
ENABLE_VECTOR_SEARCH=true
ENABLE_ASYNC_PROCESSING=true
